{
  "id": "org.freedesktop.Sdk.Extension.rust-stable",
  "branch": "1.6",
  "runtime": "org.freedesktop.Sdk",
  "build-extension": true,
  "sdk": "org.freedesktop.Sdk",
  "runtime-version": "1.6",
  "sdk-extensions": [],
  "separate-locales": false,
  "appstream-compose": false,
  "cleanup": [
    "/share/info",
    "/share/man"
  ],
  "build-options": {
    "cflags": "-O2 -g",
    "cxxflags": "-O2 -g",
    "prefix": "/usr/lib/sdk/rust-stable",
    "env": {
      "V": "1"
    }
  },
  "modules": [
    {
      "name": "rust",
      "buildsystem": "simple",
      "cleanup": [
        "/share/doc/rust/html"
      ],
      "sources": [
        {
          "type": "archive",
          "only-arches": [
            "i386"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.27.1-i686-unknown-linux-gnu.tar.gz",
          "sha256": "a0ede6e4f4f52b9b341801a98b83f58ecd5f2cd4bb84b462d5a65bf9f9f7793c"
        },
        {
          "type": "archive",
          "only-arches": [
            "arm"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.27.1-arm-unknown-linux-gnueabihf.tar.gz",
          "sha256": "da8fca48efbf761261de3a976b9e3d08558a9ecf083f387e172720523bebf687"
        },
        {
          "type": "archive",
          "only-arches": [
            "aarch64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.27.1-aarch64-unknown-linux-gnu.tar.gz",
          "sha256": "d1146b240e6f628224c3a67e3aae2a57e6c25d544115e5ece9ce91861ec92b3a"
        },
        {
          "type": "archive",
          "only-arches": [
            "x86_64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.27.1-x86_64-unknown-linux-gnu.tar.gz",
          "sha256": "435778a837af764da2a7a7fb4d386b7b78516c7dfc732d892858e9a8a539989b"
        }
      ],
      "build-commands": [
        "./install.sh --help",
        "./install.sh --list-components --verbose",
        "./install.sh --prefix=/usr/lib/sdk/rust-stable --disable-ldconfig --verbose"
      ]
    },
    {
      "name": "scripts",
      "sources": [
        {
          "type": "script",
          "commands": [
            "export PATH=$PATH:/usr/lib/sdk/rust-stable/bin"
          ],
          "dest-filename": "enable.sh"
        }
      ],
      "buildsystem": "simple",
      "build-commands": [
        "cp enable.sh /usr/lib/sdk/rust-stable/"
      ]
    },
    {
      "name": "appdata",
      "buildsystem": "simple",
      "build-commands": [
        "mkdir -p ${FLATPAK_DEST}/share/appdata",
        "cp org.freedesktop.Sdk.Extension.rust-stable.appdata.xml ${FLATPAK_DEST}/share/appdata",
        "appstream-compose  --basename=org.freedesktop.Sdk.Extension.rust-stable --prefix=${FLATPAK_DEST} --origin=flatpak org.freedesktop.Sdk.Extension.rust-stable"
      ],
      "sources": [
        {
          "type": "file",
          "path": "org.freedesktop.Sdk.Extension.rust-stable.appdata.xml"
        }
      ]
    }
  ]
}
