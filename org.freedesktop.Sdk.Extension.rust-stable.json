{
  "id": "org.freedesktop.Sdk.Extension.rust-stable",
  "branch": "1.6",
  "runtime": "org.freedesktop.Sdk",
  "build-extension": true,
  "sdk": "org.freedesktop.Sdk",
  "runtime-version": "1.6",
  "sdk-extensions": [],
  "separate-locales": false,
  "appstream-compose": false,
  "cleanup": [
    "/share/info",
    "/share/man"
  ],
  "build-options": {
    "cflags": "-O2 -g",
    "cxxflags": "-O2 -g",
    "prefix": "/usr/lib/sdk/rust-stable",
    "env": {
      "V": "1"
    }
  },
  "modules": [
    {
      "name": "rust",
      "buildsystem": "simple",
      "cleanup": [
        "/share/doc/rust/html"
      ],
      "sources": [
        {
          "type": "archive",
          "only-arches": [
            "i386"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.24.0-i686-unknown-linux-gnu.tar.gz",
          "sha256": "ad62f9bb1d9722d32de61d7f610c5ac1385cc6b29609f9b8a84027e4c3e44d62"
        },
        {
          "type": "archive",
          "only-arches": [
            "arm"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.24.0-arm-unknown-linux-gnueabihf.tar.gz",
          "sha256": "5342d8ef0d2426774470abe7539e8f412f39209aefb35b6c36428285c630c09c"
        },
        {
          "type": "archive",
          "only-arches": [
            "aarch64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.24.0-aarch64-unknown-linux-gnu.tar.gz",
          "sha256": "a981de306164b47f3d433c1d53936185260642849c79963af7e07d36b063a557"
        },
        {
          "type": "archive",
          "only-arches": [
            "x86_64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.24.0-x86_64-unknown-linux-gnu.tar.gz",
          "sha256": "336cf7af6c857cdaa110e1425719fa3a1652351098dc73f156e5bf02ed86443c"
        }
      ],
      "build-commands": [
        "./install.sh --help",
        "./install.sh --list-components --verbose",
        "./install.sh --prefix=/usr/lib/sdk/rust-stable --disable-ldconfig --verbose"
      ]
    },
    {
      "name": "scripts",
      "sources": [
        {
          "type": "script",
          "commands": [
            "export PATH=$PATH:/usr/lib/sdk/rust-stable/bin"
          ],
          "dest-filename": "enable.sh"
        }
      ],
      "buildsystem": "simple",
      "build-commands": [
        "cp enable.sh /usr/lib/sdk/rust-stable/"
      ]
    },
    {
      "name": "appdata",
      "buildsystem": "simple",
      "build-commands": [
        "mkdir -p ${FLATPAK_DEST}/share/appdata",
        "cp org.freedesktop.Sdk.Extension.rust-stable.appdata.xml ${FLATPAK_DEST}/share/appdata",
        "appstream-compose  --basename=org.freedesktop.Sdk.Extension.rust-stable --prefix=${FLATPAK_DEST} --origin=flatpak org.freedesktop.Sdk.Extension.rust-stable"
      ],
      "sources": [
        {
          "type": "file",
          "path": "org.freedesktop.Sdk.Extension.rust-stable.appdata.xml"
        }
      ]
    }
  ]
}
