{
  "id": "org.freedesktop.Sdk.Extension.rust-stable",
  "branch": "1.6",
  "runtime": "org.freedesktop.Sdk",
  "build-extension": true,
  "sdk": "org.freedesktop.Sdk",
  "runtime-version": "1.6",
  "sdk-extensions": [],
  "separate-locales": false,
  "appstream-compose": false,
  "cleanup": [
    "/share/info",
    "/share/man"
  ],
  "build-options": {
    "cflags": "-O2 -g",
    "cxxflags": "-O2 -g",
    "prefix": "/usr/lib/sdk/rust-stable",
    "env": {
      "V": "1"
    }
  },
  "modules": [
    {
      "name": "rust",
      "buildsystem": "simple",
      "cleanup": [
        "/share/doc/rust/html"
      ],
      "sources": [
        {
          "type": "archive",
          "only-arches": [
            "i386"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.24.1-i686-unknown-linux-gnu.tar.gz",
          "sha256": "a483576bb2ab237aa1ef62b66c0814f934afd8129d7c9748cb9a75da4a678c98"
        },
        {
          "type": "archive",
          "only-arches": [
            "arm"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.24.1-arm-unknown-linux-gnueabihf.tar.gz",
          "sha256": "88815f61664e86d06a6e6d11cfcce243611c8d706e0eb872a304396085f79dd7"
        },
        {
          "type": "archive",
          "only-arches": [
            "aarch64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.24.1-aarch64-unknown-linux-gnu.tar.gz",
          "sha256": "64bb25a9689b18ddadf025b90d9bdb150b809ebfb74432dc69cc2e46120adbb2"
        },
        {
          "type": "archive",
          "only-arches": [
            "x86_64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.24.1-x86_64-unknown-linux-gnu.tar.gz",
          "sha256": "4567e7f6e5e0be96e9a5a7f5149b5452828ab6a386099caca7931544f45d5327"
        }
      ],
      "build-commands": [
        "./install.sh --help",
        "./install.sh --list-components --verbose",
        "./install.sh --prefix=/usr/lib/sdk/rust-stable --disable-ldconfig --verbose"
      ]
    },
    {
      "name": "scripts",
      "sources": [
        {
          "type": "script",
          "commands": [
            "export PATH=$PATH:/usr/lib/sdk/rust-stable/bin"
          ],
          "dest-filename": "enable.sh"
        }
      ],
      "buildsystem": "simple",
      "build-commands": [
        "cp enable.sh /usr/lib/sdk/rust-stable/"
      ]
    },
    {
      "name": "appdata",
      "buildsystem": "simple",
      "build-commands": [
        "mkdir -p ${FLATPAK_DEST}/share/appdata",
        "cp org.freedesktop.Sdk.Extension.rust-stable.appdata.xml ${FLATPAK_DEST}/share/appdata",
        "appstream-compose  --basename=org.freedesktop.Sdk.Extension.rust-stable --prefix=${FLATPAK_DEST} --origin=flatpak org.freedesktop.Sdk.Extension.rust-stable"
      ],
      "sources": [
        {
          "type": "file",
          "path": "org.freedesktop.Sdk.Extension.rust-stable.appdata.xml"
        }
      ]
    }
  ]
}
