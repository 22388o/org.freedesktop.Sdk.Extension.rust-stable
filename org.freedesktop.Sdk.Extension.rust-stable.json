{
  "id": "org.freedesktop.Sdk.Extension.rust-stable",
  "branch": "1.6",
  "runtime": "org.freedesktop.Sdk",
  "build-extension": true,
  "sdk": "org.freedesktop.Sdk",
  "runtime-version": "1.6",
  "sdk-extensions": [],
  "separate-locales": false,
  "appstream-compose": false,
  "cleanup": [
    "/share/info",
    "/share/man"
  ],
  "build-options": {
    "cflags": "-O2 -g",
    "cxxflags": "-O2 -g",
    "prefix": "/usr/lib/sdk/rust-stable",
    "env": {
      "V": "1"
    }
  },
  "modules": [
    {
      "name": "rust",
      "buildsystem": "simple",
      "cleanup": [
        "/share/doc/rust/html"
      ],
      "sources": [
        {
          "type": "archive",
          "only-arches": [
            "i386"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.26.0-i686-unknown-linux-gnu.tar.gz",
          "sha256": "2aef0709b1f2e93d396143b7a926f262ac6fd24dd8768bf2c9425255a4a401c1"
        },
        {
          "type": "archive",
          "only-arches": [
            "arm"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.26.0-arm-unknown-linux-gnueabihf.tar.gz",
          "sha256": "10e1c03e8e86f9357615befa5b0ddc32cdf3c85e1e019d7225850be3b3c3da91"
        },
        {
          "type": "archive",
          "only-arches": [
            "aarch64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.26.0-aarch64-unknown-linux-gnu.tar.gz",
          "sha256": "e12dc84bdb569cdb382268a5fe6ae6a8e2e53810cb890ec3a7133c20ba8451ac"
        },
        {
          "type": "archive",
          "only-arches": [
            "x86_64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.26.0-x86_64-unknown-linux-gnu.tar.gz",
          "sha256": "13691d7782577fc9f110924b26603ade1990de0b691a3ce2dc324b4a72a64a68"
        }
      ],
      "build-commands": [
        "./install.sh --help",
        "./install.sh --list-components --verbose",
        "./install.sh --prefix=/usr/lib/sdk/rust-stable --disable-ldconfig --verbose"
      ]
    },
    {
      "name": "scripts",
      "sources": [
        {
          "type": "script",
          "commands": [
            "export PATH=$PATH:/usr/lib/sdk/rust-stable/bin"
          ],
          "dest-filename": "enable.sh"
        }
      ],
      "buildsystem": "simple",
      "build-commands": [
        "cp enable.sh /usr/lib/sdk/rust-stable/"
      ]
    },
    {
      "name": "appdata",
      "buildsystem": "simple",
      "build-commands": [
        "mkdir -p ${FLATPAK_DEST}/share/appdata",
        "cp org.freedesktop.Sdk.Extension.rust-stable.appdata.xml ${FLATPAK_DEST}/share/appdata",
        "appstream-compose  --basename=org.freedesktop.Sdk.Extension.rust-stable --prefix=${FLATPAK_DEST} --origin=flatpak org.freedesktop.Sdk.Extension.rust-stable"
      ],
      "sources": [
        {
          "type": "file",
          "path": "org.freedesktop.Sdk.Extension.rust-stable.appdata.xml"
        }
      ]
    }
  ]
}
