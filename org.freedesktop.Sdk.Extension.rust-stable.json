{
    "id": "org.freedesktop.Sdk.Extension.rust-stable",
    "branch": "19.08",
    "runtime": "org.freedesktop.Sdk",
    "build-extension": true,
    "sdk": "org.freedesktop.Sdk",
    "runtime-version": "19.08",
    "sdk-extensions": [],
    "separate-locales": false,
    "appstream-compose": false,
    "cleanup": [
        "/share/info",
        "/share/man"
    ],
    "build-options": {
        "prefix": "/usr/lib/sdk/rust-stable"
    },
    "modules": [
        {
            "name": "rust",
            "buildsystem": "simple",
            "cleanup": [
                "/share/doc/rust/html"
            ],
            "sources": [
                {
                    "type": "archive",
                    "only-arches": [
                        "i386"
                    ],
                    "url": "https://static.rust-lang.org/dist/rust-1.45.0-i686-unknown-linux-gnu.tar.gz",
                    "sha256": "38189de49bb21b18768db8479fe8cf434784d87b3df0adfedcc2a41289ef1552",
                    "x-checker-data": {
                        "type": "html",
                        "url": "https://www.rust-lang.org/",
                        "version-pattern": "Version ((?:\\d+\\.)?(?:\\d+\\.)?\\d+)",
                        "url-template": "https://static.rust-lang.org/dist/rust-$version-i686-unknown-linux-gnu.tar.gz"
                    },
                    "size": 218985239
                },
                {
                    "type": "archive",
                    "only-arches": [
                        "arm"
                    ],
                    "url": "https://static.rust-lang.org/dist/rust-1.45.0-armv7-unknown-linux-gnueabihf.tar.gz",
                    "sha256": "436a4837244eb44a0e46cb2bae16b98581d1ccb6d0de903ad35f3e3005d2e69e",
                    "x-checker-data": {
                        "type": "html",
                        "url": "https://www.rust-lang.org/",
                        "version-pattern": "Version ((?:\\d+\\.)?(?:\\d+\\.)?\\d+)",
                        "url-template": "https://static.rust-lang.org/dist/rust-$version-armv7-unknown-linux-gnueabihf.tar.gz"
                    },
                    "size": 175787526
                },
                {
                    "type": "archive",
                    "only-arches": [
                        "aarch64"
                    ],
                    "url": "https://static.rust-lang.org/dist/rust-1.45.0-aarch64-unknown-linux-gnu.tar.gz",
                    "sha256": "b727be0ecdee5fb88775b784758a09ab696293048a80288999b8a6f78b160212",
                    "x-checker-data": {
                        "type": "html",
                        "url": "https://www.rust-lang.org/",
                        "version-pattern": "Version ((?:\\d+\\.)?(?:\\d+\\.)?\\d+)",
                        "url-template": "https://static.rust-lang.org/dist/rust-$version-aarch64-unknown-linux-gnu.tar.gz"
                    },
                    "size": 171811619
                },
                {
                    "type": "archive",
                    "only-arches": [
                        "x86_64"
                    ],
                    "url": "https://static.rust-lang.org/dist/rust-1.45.0-x86_64-unknown-linux-gnu.tar.gz",
                    "sha256": "c34ed8722759fd60c94dbc9069833da5b3b873dcd19afaa9b34c1ce2c2cfa229",
                    "x-checker-data": {
                        "type": "html",
                        "url": "https://www.rust-lang.org/",
                        "version-pattern": "Version ((?:\\d+\\.)?(?:\\d+\\.)?\\d+)",
                        "url-template": "https://static.rust-lang.org/dist/rust-$version-x86_64-unknown-linux-gnu.tar.gz"
                    },
                    "size": 200994330
                }
            ],
            "build-commands": [
                "./install.sh --prefix=/usr/lib/sdk/rust-stable --disable-ldconfig --verbose"
            ]
        },
        {
            "name": "scripts",
            "sources": [
                {
                    "type": "script",
                    "commands": [
                        "export PATH=$PATH:/usr/lib/sdk/rust-stable/bin"
                    ],
                    "dest-filename": "enable.sh"
                }
            ],
            "buildsystem": "simple",
            "build-commands": [
                "cp enable.sh /usr/lib/sdk/rust-stable/"
            ]
        },
        {
            "name": "appdata",
            "buildsystem": "simple",
            "build-commands": [
                "mkdir -p ${FLATPAK_DEST}/share/appdata",
                "cp org.freedesktop.Sdk.Extension.rust-stable.appdata.xml ${FLATPAK_DEST}/share/appdata",
                "appstream-compose  --basename=org.freedesktop.Sdk.Extension.rust-stable --prefix=${FLATPAK_DEST} --origin=flatpak org.freedesktop.Sdk.Extension.rust-stable"
            ],
            "sources": [
                {
                    "type": "file",
                    "path": "org.freedesktop.Sdk.Extension.rust-stable.appdata.xml"
                }
            ]
        }
    ]
}