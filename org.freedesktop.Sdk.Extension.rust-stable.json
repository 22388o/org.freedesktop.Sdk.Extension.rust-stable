{
  "id": "org.freedesktop.Sdk.Extension.rust-stable",
  "branch": "1.6",
  "runtime": "org.freedesktop.Sdk",
  "build-extension": true,
  "sdk": "org.freedesktop.Sdk",
  "runtime-version": "1.6",
  "sdk-extensions": [],
  "separate-locales": false,
  "appstream-compose": false,
  "cleanup": [
    "/share/info",
    "/share/man"
  ],
  "build-options": {
    "cflags": "-O2 -g",
    "cxxflags": "-O2 -g",
    "prefix": "/usr/lib/sdk/rust-stable",
    "env": {
      "V": "1"
    }
  },
  "modules": [
    {
      "name": "rust",
      "buildsystem": "simple",
      "cleanup": [
        "/share/doc/rust/html"
      ],
      "sources": [
        {
          "type": "archive",
          "only-arches": [
            "i386"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.26.1-i686-unknown-linux-gnu.tar.gz",
          "sha256": "d704ba5cbaaf93c5e2112d211630db0e460d5dc819a43464ba91581e5c821df3"
        },
        {
          "type": "archive",
          "only-arches": [
            "arm"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.26.1-arm-unknown-linux-gnueabihf.tar.gz",
          "sha256": "32eace8f285a6f334899be47ff3ded94322b5af8b68e1e0ca94cb8ec4741db06"
        },
        {
          "type": "archive",
          "only-arches": [
            "aarch64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.26.1-aarch64-unknown-linux-gnu.tar.gz",
          "sha256": "d4a369053c2dfd5f457de6853557dab563944579fa4bb55bc919bacf259bff6d"
        },
        {
          "type": "archive",
          "only-arches": [
            "x86_64"
          ],
          "url": "https://static.rust-lang.org/dist/rust-1.26.1-x86_64-unknown-linux-gnu.tar.gz",
          "sha256": "b7e964bace1286696d511c287b945f3ece476ba77a231f0c31f1867dfa5080e0"
        }
      ],
      "build-commands": [
        "./install.sh --help",
        "./install.sh --list-components --verbose",
        "./install.sh --prefix=/usr/lib/sdk/rust-stable --disable-ldconfig --verbose"
      ]
    },
    {
      "name": "scripts",
      "sources": [
        {
          "type": "script",
          "commands": [
            "export PATH=$PATH:/usr/lib/sdk/rust-stable/bin"
          ],
          "dest-filename": "enable.sh"
        }
      ],
      "buildsystem": "simple",
      "build-commands": [
        "cp enable.sh /usr/lib/sdk/rust-stable/"
      ]
    },
    {
      "name": "appdata",
      "buildsystem": "simple",
      "build-commands": [
        "mkdir -p ${FLATPAK_DEST}/share/appdata",
        "cp org.freedesktop.Sdk.Extension.rust-stable.appdata.xml ${FLATPAK_DEST}/share/appdata",
        "appstream-compose  --basename=org.freedesktop.Sdk.Extension.rust-stable --prefix=${FLATPAK_DEST} --origin=flatpak org.freedesktop.Sdk.Extension.rust-stable"
      ],
      "sources": [
        {
          "type": "file",
          "path": "org.freedesktop.Sdk.Extension.rust-stable.appdata.xml"
        }
      ]
    }
  ]
}
